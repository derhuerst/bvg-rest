{
  "version": 2,
  "builds": [{ "src": "index.js", "use": "@now/node@latest" }],
  "routes": [
    {
      "src": "/docs",
      "dest": "/index.js",
      "methods": ["GET", "OPTIONS"]
    },
    {
      "src": "/stops/nearby",
      "dest": "/index.js",
      "methods": ["GET", "OPTIONS"]
    },
    {
      "src": "/stations/",
      "dest": "/index.js",
      "methods": ["GET", "OPTIONS"]
    },
    {
      "src": "/stops/:id",
      "dest": "/index.js",
      "methods": ["GET", "OPTIONS"]
    },
    {
      "src": "/stations/(.*)/departures",
      "dest": "/index.js",
      "methods": ["GET", "OPTIONS"]
    },
    {
      "src": "/journeys",
      "dest": "/index.js",
      "methods": ["GET", "OPTIONS"]
    },
    {
      "src": "/journeys/(.*)",
      "dest": "/index.js",
      "methods": ["GET", "OPTIONS"]
    },
    {
      "src": "/trips/(.*)",
      "dest": "/index.js",
      "methods": ["GET", "OPTIONS"]
    },
    {
      "src": "/locations",
      "dest": "/index.js",
      "methods": ["GET", "OPTIONS"]
    },
    {
      "src": "/radar",
      "dest": "/index.js",
      "methods": ["GET", "OPTIONS"]
    },
    {
      "src": "/stops",
      "dest": "/index.js",
      "methods": ["GET", "OPTIONS"]
    }
  ]
}
